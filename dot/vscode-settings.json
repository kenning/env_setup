{
  "editor.selectionClipboard": false,
  "editor.codeActionsOnSave": {
    // "source.fixAll": true
  },
  "editor.tabSize": 2,
  "[markdown]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "editor.rulers": [100],
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "keyboard.dispatch": "keyCode",
  "editor.formatOnSave": true,
  "emmet.showExpandedAbbreviation": "never",
  "editor.suggest.showSnippets": false,
  "vim.cursorStylePerMode.insert": "line",
  "vim.handleKeys": {
    "<C-d>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-w>": false
  },
  "vim.cursorStylePerMode.replace": "block-outline",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.indentSize": 4,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "window.autoDetectHighContrast": false,
  "go.lintOnSave": "workspace",
  "go.lintTool": "golangci-lint",
  "go.formatTool": "gofmt",
  "go.useLanguageServer": true,
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "go.docsTool": "gogetdoc",
  "javascript.suggest.completeJSDocs": false,
  "editor.autoClosingOvertype": "never",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "typescript.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "files.associations": {
    "*.go": "go",
    "*.mdx": "markdown"
  },
  "go.lintFlags": ["--timeout 5m", " --disable=typecheck", "--sort-results "],
  "files.exclude": {
    "web/build": true,
    "web/node_modules": true
  },
  "editor.quickSuggestions": {
    "text": false,
    "snippet": false,
    "comments": false,
    "strings": false
  },
  "files.watcherExclude": {
    "**/.git/**": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "Gruvbox Light Soft",
  "go.toolsManagement.checkForUpdates": "off",
  "eslint.alwaysShowStatus": true,
  "eslint.lintTask.options": "",
  "terminal.integrated.inheritEnv": false,
  "editor.defaultFormatter": "ms-dotnettools.csharp",
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "godot_tools.reconnect_attempts": 3,
  "godot_tools.reconnect_cooldown": 2000,
  "godot.csharp.executablePath": "",
  "godot_tools.editor_path": "/home/nick/godot_stuff/Godot_v4.0.2-stable_linux.x86_64",
  "godot_tools.gdscript_lsp_server_port": 6005,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "git.openRepositoryInParentFolders": "never",
  "prettier.ignorePath": "",
  "python.defaultInterpreterPath": "/home/nick/anaconda3",
  "window.zoomLevel": -1,
  "workbench.editor.empty.hint": "hidden",
  "remote.SSH.defaultExtensions": ["ms-toolsai.jupyter", "ms-python.python"],
  "explorer.confirmDragAndDrop": false
}
